#include<bits/stdc++.h>
using namespace std;
int main() {
  double A, B, C;
  cin>> A >> B >> C;
  double pi = 3.14159;
  double triangle, circle, trapezium, square, rectangle;
  triangle = (A*C)/2.0;
  circle= pi * C * C;
  trapezium = ((A+B)* C) /2.0;
  square = B * B;
  rectangle = A * B;

    cout << fixed << setprecision(3);
    cout << "TRIANGULO: " << triangle << endl;
    cout << "CIRCULO: " << circle << endl;
    cout << "TRAPEZIO: " << trapezium << endl;
    cout << "QUADRADO: " << square << endl;
    cout << "RETANGULO: " << rectangle << endl;
}
